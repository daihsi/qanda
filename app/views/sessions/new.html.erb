<h1>
  ログイン
</h1>
<%= form_with url: login_path, local: true do |form| %>
  <%= form.label 'メールアドレス'%><br>
  <%= form.email_field :email%><br>
  <%= form.label 'パスワード'%><br>
  <%= form.password_field :password%><br>
  <%= form.submit 'ログイン' %>
<% end %><br>
<%= link_to 'ユーザー登録ページへ', users_new_path %>
